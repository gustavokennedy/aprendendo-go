# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Video {
  id: ID!
  title: String!
  url: String!
  author: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  videos: [Video!]!
}

input NewVideo {
  title: String!
  url: String!
  userId: String!
}

type Mutation {
  createVideo(input: NewVideo!): Video!
}

type Subscription {
  videoAdded(repoFullName: String!): Video
}
